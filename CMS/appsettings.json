{
  "ActiveDatabase": "SqlServer", // Possible values: LocalDb, SqlServer, InMemoryDatabase, SQLite
  "ConnectionStrings": {
    "SqlServer": {
      "ApplicationDbContextConnection": "Data Source=.;Initial Catalog=CMS;Persist Security Info=True;Integrated Security=True;"
    },
    "LocalDb": {
      "InitialCatalog": "CMS",
      "AttachDbFilename": "CMS.mdf"
    },
    "SQLite": {
      "ApplicationDbContextConnection": "Data Source=|DataDirectory|\\CMS.sqlite"
    }
  },

  "CookieOptions": {
    "AccessDeniedPath": "/Error/Index/401",
    "CookieName": ".ASPNETCoreIdentity.Rashin",
    "ExpireTimeSpan": "15.00:00:00.0000",
    "LoginPath": "/Login",
    "LogoutPath": "/Login/LogOff",
    "SlidingExpiration": true,
    "UseDistributedCacheTicketStore": true,
    "DistributedSqlServerCacheOptions": {
      "ConnectionString": "",
      "TableName": "AppSqlCache",
      "SchemaName": "dbo"
    },
    "DistributedSqliteCacheOptions": {
      "ConnectionString": "Data Source=|DataDirectory|\\cache.sqlite"
    }
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "EnableEmailConfirmation": true,
  "EmailConfirmationTokenProviderLifespan": "7.00:00:00.0000",
  "NotAllowedPreviouslyUsedPasswords": 5,
  "ChangePasswordReminderDays": 90,

  "DataProtectionOptions": {
    "ApplicationName": ".ASPNETCoreIdentity.Rashin",
    "DataProtectionKeyLifetime": "15.00:00:00.0000"
  },

  "PasswordOptions": {
    "RequireDigit": false,
    "RequiredLength": 6,
    "RequireLowercase": false,
    "RequireNonAlphanumeric": false,
    "RequireUppercase": false
  },

  "LockoutOptions": {
    "AllowedForNewUsers": true,
    "DefaultLockoutTimeSpan": "0.00:05:00.0000",
    "MaxFailedAccessAttempts": 5
  },

  "EmailsBanList": [
    "yandex.com",
    "spamdecoy.net",
    "wavemail.ir",
    "mailinator.com",
    "trbvm.com",
    "emeil.com",
    "chmail.com",
    "30mail.ir",
    "gmx.de",
    "walkmail.net",
    "temp-mail.org",
    "emeil.ir",
    "techgroup.me",
    "sharklasers.com",
    "my.com",
    "emailirani.ir",
    "rocketmail.com",
    "inboxdesign.me",
    "ymail.com",
    "lormail.ir",
